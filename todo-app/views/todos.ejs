<table>
    <tr>
        <th>SNo</th>
        <th>To-Do Item</th>
        <th>Due Date</th>
    </tr>
    <% for( var i=0; i < allTodos.length; i++) { %>
        <tr>
            <td><%= allTodos[i].id %></td>
            <td><%= allTodos[i].title %></td>
            <td><%= allTodos[i].dueDate %></td>
        </tr> 
    <% } %>
</table>
<br>
